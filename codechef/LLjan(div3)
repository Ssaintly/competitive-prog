#Ques.n Code: DIVTHREE
Sol.n-
#include<bits/stdc++.h>
#include<math.h>
#include<set>
#include<string>
using namespace std;

#define int                long long int
#define fast_io            ios_base::sync_with_stdio(false);cin.tie(0);cout.tie(0);
#define pb                 push_back
#define mk                 make_pair
#define R                  return
#define ln                 "\n"
#define ull                unsigned long long
#define all(x)             x.begin(),x.end()
#define w(x)               int x;cin>>x;for(int i=1;i<=x;i++)
#define vi                 vector<pair<int,int>>
#define ll                 long long
#define fi                 first
#define se                 second


void solve(int input)
{
    int n, d, k;
    cin >> n >> d >> k;
    int t_ques = 0;
    if (n == 1)
    {
        int a; cin >> a;
        int days = a / d;
        if (days > k)
            days = k;
        cout << days << ln;
        R;
    }
    for (int i = 0; i < n; i++)
    {
        int a;
        cin >> a;
        t_ques += a;
    }
    int days = t_ques / d;
    if (days > k)
        days = k;
    cout << days << ln;
    R;
}

signed main()
{
    fast_io
    int x = 1;
    cin >> x;
    for (int i = 1; i <= x; i++)
        solve(i);
    return 0;
}
