**Ques_n code:1426B
#include<bits/stdc++.h>
#include<math.h>
#include<set>
#include<string>
using namespace std;

//#define int                long long int
#define fast_io            ios_base::sync_with_stdio(false);cin.tie(0);cout.tie(0);
#define pb                 push_back
#define mk                 make_pair
#define R                  return
#define ln                 "\n"
#define ull                unsigned long long
#define all(x)             x.begin(),x.end()
#define w(x)               int x;cin>>x;for(int i=1;i<=x;i++)
#define vi                 vector<pair<int,int>>
#define ll                 long long
#define fi                 first
#define se                 second


void solve(int input)
{
    // cout << input << " ";
    int n;
    cin >> n;
    string s;
    cin >> s;
    if (s[0] == '2')
    {
        if (s[1] == '0')
        {
            if (s[2] == '2')
            {
                if (s[3] == '0' || s[n - 1] == '0')
                {
                    cout << "YES" << ln; R;
                }
            }
            if (s[n - 2] == '2' && s[n - 1] == '0')
            {
                cout << "YES" << ln; R;
            }
        }
        if (s[n - 3] == '0' && s[n - 2] == '2' && s[n - 1] == '0')
        {
            cout << "YES" << ln; R;
        }
    }
    if (s[n - 4] == '2' && s[n - 3] == '0' && s[n - 2] == '2' && s[n - 1] == '0')
    {cout << "YES" << ln; R;}
    cout << "NO" << ln; R;
}

signed main()
{
    fast_io
    int x = 1;
    cin >> x;
    for (int i = 1; i <= x; i++)
        solve(i);
    return 0;
}

