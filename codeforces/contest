#round 690(div3)
**Ques_n code:1462A
#include<bits/stdc++.h>
#include<math.h>
#include<set>
#include<string>
using namespace std;

//#define int                long long int
#define fast_io            ios_base::sync_with_stdio(false);cin.tie(0);cout.tie(0);
#define pb                 push_back
#define mk                 make_pair
#define R                  return
#define ln                 "\n"
#define ull                unsigned long long
#define all(x)             x.begin(),x.end()
#define w(x)               int x;cin>>x;for(int i=1;i<=x;i++)
#define vi                 vector<pair<int,int>>
#define ll                 long long
#define fi                 first
#define se                 second


void solve(int input)
{
    ll int n;
    cin >> n;
    ll int ar[n];
    for (ll int i = 0; i < n; i++)
        cin >> ar[i];
    if (n == 1)
    {cout << ar[0] << ln; R;}
    for (ll int i = 0; i < n / 2; i++)
    {
        cout << ar[i] << " " << ar[n - 1 - i] << " ";
    }
    if (n % 2 != 0 && n > 2)
        cout << ar[n / 2] << ln;
    else
        cout << ln;
    return;

}

signed main()
{
    fast_io
    int x = 1;
    cin >> x;
    for (int i = 1; i <= x; i++)
        solve(i);
    return 0;
}
